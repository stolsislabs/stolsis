{
  "name": "react-example",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen",
    "format": "prettier --write ./src",
    "gen:dojo": "pnpm gen:models && pnpm gen:events && pnpm format",
    "gen:models": "npx create-models ../contracts/manifests/dev/manifest.json ./src/dojo/generated/contractModels.ts",
    "gen:events": "npx create-events ../contracts/manifests/dev/manifest.json ./src/dojo/generated/contractEvents.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "bin": {
    "create-models": "./bin/generateModels.cjs",
    "create-events": "./bin/generateEvents.cjs"
  },
